!go limbo

!new steve male character noun_steve
!set steve.name Steve
!set steve.description "A dead-ringer for Salman Rushdie."
!set steve.start_container $charterhouse_square_room

!new jim male character noun_james
!set jim.name Jim
!set jim.description "A friendly looking chap."
!set jim.start_container $charterhouse_square_room

!new matt male character noun_matthew
!set matt.name Matt
!set matt.description "A friendly looking chap."
!set matt.start_container $charterhouse_square_room

!new alex male character noun_alexander
!set alex.name Alex
!set alex.description "A funny looking chap."
!set alex.start_container $charterhouse_square_room

!new lori female character noun_lori
!set lori.name Lori
!set lori.description "A friendly looking gal."
!set lori.start_container $charterhouse_square_room


!new will male character noun_william
!set will.name Will
